{
  "name": "mock-together",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "api-gen-mock": "rm -R ./src/app/api/mock && openapi-generator-cli generate -g typescript-angular -i ./mock-openapi.yaml -o ./src/app/api/mock --additional-properties=useSingleRequestParameter=true,apiModulePrefix=MockModule,modelSuffix=Mock,serviceSuffix=MockService,enumNameSuffix=Mock",
    "api:mock": "npm run api-gen-mock && redocly build-docs mock-openapi.yaml -o oas-docs/index.html && docker compose -f docker-compose-local.yml up --build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@openapitools/openapi-generator-cli": "^2.15.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.7",
    "@angular/cli": "^17.3.7",
    "@angular/compiler-cli": "^17.3.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.4.2",
    "@redocly/cli": "^1.25.3"
  }
}
